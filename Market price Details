import java.util.Scanner;
public class Price {
    public static void main (String[] args){
        
        int Product=1;  
        Scanner inpu = new Scanner (System .in);
        System.out.println("Enter number");
        int number =inpu.nextInt();
        inpu.close();
        while(number > 0){
         int Num = number %10;
         Product = Product * Num;
          number = number / 10;
          
        }
        System.out.println(Product);
}
}
